<%- include('partials/header.ejs') -%>
  </head>
  <body>
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-6 col-md-offset-3 text-center">
        <div class="btn-group" data-toggle="buttons">
        <label class="btn btn-primary active">
          <input type="radio" name="time" id="hour" value="hour" autocomplete="off" checked> Hour
        </label>
        <label class="btn btn-primary">
          <input type="radio" name="time" id="day" value="day" autocomplete="off"> Day
        </label>
        <label class="btn btn-primary">
          <input type="radio" name="time" id="week" value="week" autocomplete="off"> Week
        </label>
      </div>
      <br>
      <span id="eq-last"></span>
    </div>
  </div>
</div>
  </body>
  <script type="text/javascript" src="/javascripts/recent_earthquakes.js"></script>
